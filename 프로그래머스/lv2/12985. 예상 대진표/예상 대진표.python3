def solution(n,a,b):
    answer = 1
    
    while True:     
        if a % 2 == 1:
            a += 1
        if b % 2 == 1:
            b += 1  
            
        a /= 2
        b /= 2
        
        if abs(a-b) == 0:
            break 
            
        answer += 1
        
    return answer